{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "base-cms": {
      "projectType": "library",
      "root": "projects/base-cms",
      "sourceRoot": "projects/base-cms/src",
      "prefix": "base-cms",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/base-cms/ng-package.json",
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/base-cms/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/base-cms/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/base-cms/src/test.ts",
            "tsConfig": "projects/base-cms/tsconfig.spec.json",
            "karmaConfig": "projects/base-cms/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": ["projects/base-cms/**/*.ts", "projects/base-cms/**/*.html"]
          }
        }
      }
    },
    "safe-drive": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "projects/safe-drive",
      "sourceRoot": "projects/safe-drive/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/safe-drive",
            "index": "projects/safe-drive/src/index.html",
            "main": "projects/safe-drive/src/main.ts",
            "polyfills": "projects/safe-drive/src/polyfills.ts",
            "tsConfig": "projects/safe-drive/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": ["projects/safe-drive/src/favicon.ico", "projects/safe-drive/src/assets"],
            "styles": ["projects/safe-drive/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "projects/safe-drive/src/environments/environment.ts",
                  "with": "projects/safe-drive/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "safe-drive:build:production"
            },
            "development": {
              "browserTarget": "safe-drive:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "safe-drive:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/safe-drive/src/test.ts",
            "polyfills": "projects/safe-drive/src/polyfills.ts",
            "tsConfig": "projects/safe-drive/tsconfig.spec.json",
            "karmaConfig": "projects/safe-drive/karma.conf.js",
            "inlineStyleLanguage": "scss",
            "assets": ["projects/safe-drive/src/favicon.ico", "projects/safe-drive/src/assets"],
            "styles": ["projects/safe-drive/src/styles.scss"],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": ["projects/safe-drive/**/*.ts", "projects/safe-drive/**/*.html"]
          }
        }
      }
    }
  },
  "defaultProject": "base-cms",
  "cli": {
    "defaultCollection": "@angular-eslint/schematics"
  }
}
